{
    "name": "pokeserver",
    "version": "1.0.0",
    "description": "Your application description",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "start": "npx knex migrate:latest && node index.js",
        "test": "jest"
    },
    "jest": {
        "testEnvironment": "node",
        "moduleFileExtensions": [
            "js",
            "json"
        ],
        "roots": [
            "<rootDir>"
        ],
        "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.js?$",
        "transform": {
            "^.+\\.js?$": "babel-jest"
        },
        "moduleNameMapper": {
            "^@/(.*)$": "<rootDir>/$1"
        },
        "collectCoverage": true,
        "collectCoverageFrom": [
            "**/*.{js}",
            "!**/node_modules/**",
            "!**/vendor/**"
        ],
        "coverageDirectory": "<rootDir>/coverage",
        "coverageReporters": [
            "json",
            "lcov",
            "text",
            "clover"
        ]
    },
    "keywords": [],
    "author": "is-paranoia",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "2.4.3",
        "cors": "2.8.5",
        "dotenv": "16.3.1",
        "express": "4.17.1",
        "jsonwebtoken": "^9.0.2",
        "knex": "^3.0.1",
        "pg": "^8.11.3",
        "redis": "4.6.10"
    },
    "devDependencies": {
        "@babel/core": "^7.23.5",
        "@babel/plugin-transform-modules-commonjs": "^7.23.3",
        "@babel/preset-env": "^7.23.5",
        "babel-jest": "^29.7.0",
        "jest": "^29.7.0",
        "supertest": "^6.3.3"
    }
}
